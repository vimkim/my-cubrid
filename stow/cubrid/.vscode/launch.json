{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "gdbserver, general",
      "type": "cppdbg",
      "request": "launch",
      "program": "/proc/${command:pickProcess}/exe",
      "miDebuggerServerAddress": "localhost:9999",
      "miDebuggerPath": "gdb",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "gdbserver, csql",
      "type": "cppdbg",
      "request": "launch",
      "program": "csql",
      "miDebuggerServerAddress": "localhost:9999",
      "miDebuggerPath": "gdb",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "type": "cppdbg",
      "request": "launch",
      "name": "core, csql",
      "program": "${env:CUBRID}/bin/csql",
      "coreDumpPath": "${workspaceFolder}/${input:coreFileName}",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb"
    },
    {
      "name": "(gdb) csql 연결",
      "type": "cppdbg",
      "request": "attach",
      "program": "${env:CUBRID}/bin/csql",
      "processId": "${command:pickProcess}",
      // "processId": "${input:findCsql}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "engineLogging": "warning"
      }
    },
    {
      "name": "(gdb) cub_cas 연결",
      "type": "cppdbg",
      "request": "attach",
      "program": "${env:CUBRID}/bin/cub_cas",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) cub_server 연결",
      "type": "cppdbg",
      "request": "attach",
      "program": "${env:CUBRID}/bin/cub_server",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) cubrid 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid",
      "args": [
        "createdb",
        "-F",
        "${env:CUBRID_DATABASES}/demodb",
        "-L",
        "${env:CUBRID_DATABASES}/demodb/log",
        "--db-volume-size=64M",
        "--log-volume-size=64M",
        "demodb",
        "ko_KR.utf8"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) loaddb schema 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid",
      "args": [
        "loaddb",
        "-u",
        "dba",
        "-s",
        "${env:HOME}/loaddb/demo/demodb_schema",
        "-v",
        "demodb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) loaddb object 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid",
      "args": [
        "loaddb",
        "-u",
        "dba",
        "-d",
        "${env:HOME}/loaddb/demo/demodb_objects",
        "demodb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) mdb loaddb schema 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid",
      "args": [
        "loaddb",
        "-u",
        "dba",
        "-s",
        "${env:HOME}/loaddb/mdb_schema",
        "-v",
        "demodb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) mdb loaddb object 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid",
      "args": [
        "loaddb",
        "-u",
        "dba",
        "-d",
        "${env:HOME}/loaddb/mdb_objects",
        "demodb"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) csql 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/csql",
      "args": ["-u", "dba", "demodb"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) csql -S 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/csql",
      "args": ["-u", "dba", "demodb", "-S"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) cubrid_replay 시작",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cubrid_replay",
      "args": [
        "-I",
        "localhost",
        "-P",
        "33000",
        "-d",
        "demodb",
        "-u",
        "public",
        "${env:HOME}/work/20231127/log/broker/sql_log/broker1_1.sql.log",
        "${env:HOME}/work/20231127/log/broker/result.out"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) cub_server with core file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:CUBRID}/bin/cub_server",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "gdb에 자동 서식 지정 사용",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "coreDumpPath": "${env:HOME}/work/20231127/core.97749"
    }
  ],
  "inputs": [
    {
      "id": "findCsql",
      "type": "command",
      "command": "shellCommand.execute",
      "args": {
        "command": "pgrep csql",
        "description": "From: https://stackoverflow.com/questions/65723608/vs-code-debugging-is-it-possible-to-supply-process-id-pgrep-x-myprog-for",
        "useFirstResult": true
      }
    },
    {
      "id": "coreFileName",
      "type": "promptString",
      "description": "Enter core file path"
    }
  ]
}
